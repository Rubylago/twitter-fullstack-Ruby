<div class="col-md-3 offset-md-9">
  <h4>Popular</h4>
    {{#each users}}
    <blockquote class="blockquote mb-0">
      <img src="{{ this.avatar }}" alt="user-picture" class="rounded-circle">
      <span><a href="/users/{{ this.UserId }}">{{ this.User.name }}</a></span>
      <span class="blockquote-footer"> @{{ this.User.account }}</span>
    </blockquote>
    <hr />
    {{/each}}
</div>
